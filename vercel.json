{
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "outputDirectory": "dist",
    "functions": {
        "api/*.ts": {
            "memory": 128,
            "maxDuration": 25
        }
    }
}